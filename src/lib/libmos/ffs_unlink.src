	.assume	adl=1
	.include "agon/mos.inc"
	.section .text
	.global	_ffs_unlink
_ffs_unlink:
    pop     de
    ex      (sp), hl            ; Pointer to a zero-terminated file path string
    push    de
    
    ld      a,  ffs_unlink
    rst.lil 08h                 ; returns FRESULT in A

    ret
    
