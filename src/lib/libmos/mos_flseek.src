	.assume	adl =1
	.include "agon/mos.inc"
	.section .text
	.global  _mos_flseek
_mos_flseek:
	push	ix
	ld 	ix, 0
	add 	ix, sp
	ld 	bc, (ix+6)			; file handle
	ld	de, 0
	ld	hl, (ix+9) 			; 24 least significant bits
	ld	e, (ix+12)			; 8 most most significant bits
	ld a,	mos_flseek
	rst.lil	08h
	pop	ix
	ret
