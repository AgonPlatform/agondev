	.assume	adl=1
	.include "agon/mos.inc"
	.section .text
	.global	_ffs_ftruncate
_ffs_ftruncate:
    pop     de
    ex      (sp), hl            ; Pointer to a FIL structure
    push    de
    
    ld      a,  ffs_ftruncate
    rst.lil 08h                 ; returns FRESULT in A

    pop     ix
    ret
    
