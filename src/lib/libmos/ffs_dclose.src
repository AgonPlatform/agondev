    .assume    adl =1
    .include "agon/mos.inc"
    .section .text
    .global  _ffs_dclose
_ffs_dclose:
    push	ix
    ld      ix, 0
    add     ix, sp

    ld      hl, (ix+6)            ; pointer to DIR handle
    ld      a,  ffs_dclose
    rst.lil 08h                   ; returns FRESULT in A

    ld      sp, ix
    pop     ix
    ret        
