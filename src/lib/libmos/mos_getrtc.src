	.assume	adl =1
	.include "agon/mos.inc"
	.section .text
	.global  _mos_getrtc
_mos_getrtc:
  pop   de
  ex    (sp), hl    ; Pointer to a buffer to copy the string to (at least 32 bytes)
  push  de
	ld    a,	mos_getrtc
	rst.lil	08h				; Get a time string from the RTC (Requires MOS 1.03 or above)
	ret
