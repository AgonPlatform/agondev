	.assume	adl=1
	.include "agon/mos.inc"
	.section .text
	.global	_sd_readblocks
_sd_readblocks:
    ld      c,  0
    ld      b,  0x01            ; sd_readblocks - BYTE SD_readBlocks(DWORD sector, BYTE *buf, WORD count);
    ld      a,  mos_getfunction
    rst.lil 08h
    or      a,  a
    ret     nz                  ; on error
    jp      (hl)
