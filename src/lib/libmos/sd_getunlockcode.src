	.assume	adl=1
	.include "agon/mos.inc"
	.section .text
	.global	_sd_getunlockcode
_sd_getunlockcode:
    pop     de
    ex      (sp), hl            ; Pointer to a 24-bit value to store the unlock code
    push    de
    ld      a,  sd_getunlockcode
    rst.lil 08h

    ld      hl, (hl)            ; Returns 24-bit unlock code in HLU
    ret
    
