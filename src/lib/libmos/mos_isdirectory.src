	.assume	adl=1
	.include "agon/mos.inc"
	.section .text
	.global	_mos_isdirectory
_mos_isdirectory:
    pop     de
    ex      (sp), hl            ; Pointer to the path to check
    push    de
    ld      a,  mos_isdirectory
    rst.lil 08h                 ; Returns A: Status code (0 = Success, 5 = No path)

    ret
    
