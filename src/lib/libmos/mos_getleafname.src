	.assume	adl=1
	.include "agon/mos.inc"
	.section .text
	.global	_mos_getleafname
_mos_getleafname:
    pop     de
    ex      (sp), hl            ; Pointer to the path to get the leafname from
    push    de
    ld      a,  mos_getleafname
    rst.lil 08h                 ; returns pointer to the leafname (HLU)
    ret
    
