	.assume	adl=1
	.include "agon/mos.inc"
	.section .text
	.global	_ffs_fputc
_ffs_fputc:
    push    ix
    ld      ix, 0
    add     ix, sp
    
    ld      hl, (ix+6)          ; Pointer to a FIL structure
    ld      c,  (ix+9)          ; Character to write
    ld      a,  ffs_fputc
    rst.lil 08h

    push    bc
    pop     hl                  ; Returns number of bytes written
    pop     ix
    ret
    
