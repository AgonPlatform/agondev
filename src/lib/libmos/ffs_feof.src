	.assume	adl=1
	.include "agon/mos.inc"
	.section .text
	.global	_ffs_feof
_ffs_feof:
    pop     de
    ex      (sp), hl            ; Pointer to a FIL structure
    push    de
    
    ld      a,  ffs_feof
    rst.lil 08h                 ; returns A: 1 if at the end of the file, otherwise 0

    ret
    
