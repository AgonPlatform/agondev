	.assume	adl=1
	.include "agon/mos.inc"
	.section .text
	.global	_ffs_fprintf
_ffs_fprintf:
    ld      c,  0
    ld      b,  0x05            ; ffs_fprintf - int f_printf (FIL* fp, const TCHAR* str, ...);
    ld      a,  mos_getfunction
    rst.lil 08h
    or      a,  a
    ret     nz                  ; on error
    jp      (hl)
