	.assume	adl =1
	.include "agon/mos.inc"
	.section .text
	.global	_mos_puts
_mos_puts:
	push	ix
	ld 	ix,0
	add 	ix, sp
	ld 	hl, (ix+6)			; Address of buffer
	ld	bc, (ix+9)			; Size to write from buffer - or 0 .if using delimiter
	ld	a, (ix+12) 			; delimiter - only .if size is 0
	rst.lil	18h				; Write a block of bytes out to the ESP32
	pop	ix
	ret
