	.assume	adl=1
	.include "agon/mos.inc"
	.section .text
	.global	_mos_waitforvdpflags
_mos_waitforvdpflags:
    pop     de
    ex      (sp), hl
    push    de
    ld      c, l           ; Bitmask of flags to wait for
    ld      a,  mos_waitforvdpflags
    rst.lil 08h            ; returns A: Status code (0 = Success, 15 = Timeout)
    ret
    
