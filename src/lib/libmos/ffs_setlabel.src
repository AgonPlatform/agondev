	.assume	adl=1
	.include "agon/mos.inc"
	.section .text
	.global	_ffs_setlabel
_ffs_setlabel:
    pop     de
    ex      (sp), hl            ; Pointer to a zero-terminated volume label string
    push    de
    
    ld      hl, (ix+6)          ; Pointer to a zero-terminated volume label string
    ld      a,  ffs_setlabel
    rst.lil 08h                 ; returns FRESULT in A

    ret
    
